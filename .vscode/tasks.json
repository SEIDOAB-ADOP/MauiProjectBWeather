{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-maccatalyst",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/MauiProjectBWeather/MauiProjectBWeather.csproj",
        "-f",
        "net9.0-maccatalyst",
        "-c",
        "Debug"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "build-windows-arm64",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/MauiProjectBWeather/MauiProjectBWeather.csproj",
        "-f",
        "net9.0-windows10.0.19041.0",
        "-r",
        "win10-arm64",
        "-c",
        "Debug"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "build-windows-x64",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/MauiProjectBWeather/MauiProjectBWeather.csproj",
        "-f",
        "net9.0-windows10.0.19041.0",
        "-r",
        "win10-x64",
        "-c",
        "Debug"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    }
  ]
}
